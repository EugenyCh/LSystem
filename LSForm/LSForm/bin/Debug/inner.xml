<?xml version="1.0" encoding="UTF-8" ?>
<templates>
  <fractal name="3D Hilbert Curve">
    <axiom>X</axiom>
    <rule char="X">^\XF^\XFX-F^//XFX&amp;F+//XFX-F/X-/</rule>
  </fractal>

  <fractal name="Triangle">
    <axiom>F+F+F</axiom>
    <rule char="F">F-F+F</rule>
    <angle>120</angle>
  </fractal>

  <fractal name="Quadratic Gosper">
    <axiom>-YF</axiom>
    <rule char="X">XFX-YF-YF+FX+FX-YF-YFFX+YF+FXFXYF-FX+YF+FXFX+YF-FXYF-YF-FX+FX+YFYF-</rule>
    <rule char="Y">+FXFX-YF-YF+FX+FXYF+FX-YFYF-FX-YF+FXYFYF-FX-YFFX+FX+YF-YF-FX+FX+YFY</rule>
  </fractal>

  <fractal name="Square Sierpinski">
    <axiom>F+XF+F+XF</axiom>
    <rule char="X">XF-F+F-XF+F+XF-F+F-X</rule>
  </fractal>

  <fractal name="Crystal">
    <axiom>F+F+F+F</axiom>
    <rule char="F">FF+F++F+F</rule>
  </fractal>

  <fractal name="Peano Curve">
    <axiom>X</axiom>
    <rule char="X">XFYFX+F+YFXFY-F-XFYFX</rule>
    <rule char="Y">YFXFY-F-XFYFX+F+YFXFY</rule>
  </fractal>

  <fractal name="Quadratic Snowflake">
    <axiom>F</axiom>
    <rule char="F">F-F+F+F-F</rule>
  </fractal>
</templates>